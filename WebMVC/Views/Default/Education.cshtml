@using WebMVC.Models.Entity;
@model List<Education>


@foreach (var item in Model)
{
        <div class="position-relative overflow-hidden p-3 p-md-5 m-md-3 text-center text-light">
            <div class="col-md-5 p-lg-5 mx-auto my-5">
                <h1 class="display fw-normal">@item.SchoolName</h1>
                <span style="float: left">@item.SchoolTitle</span>
                <span style="float:right">@item.SchoolDate</span>
                <br />
                <hr />
                <p class="lead fw-normal">@item.SchoolDescription</p>
            </div>
            <div class="product-device shadow-sm d-none d-md-block"></div>
            <div class="product-device product-device-2 shadow-sm d-none d-md-block"></div>
        </div>
}