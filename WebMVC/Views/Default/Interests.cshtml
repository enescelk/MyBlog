@using WebMVC.Models.Entity;
@model List<Interest>

@foreach (var item in Model)
{
    <div class="position-relative overflow-hidden p-3 p-md-5 m-md-3 text-center text-light">
        <div class="col-md-5 p-lg-5 mx-auto my-5">
            <h1 class="display fw-normal">Hobilerim</h1>
            <hr />
            <p class="lead fw-normal">@item.InteretsDescription</p>
            <p class="lead fw-normal">@item.InteretsTwoDescription</p>
        </div>
        <div class="product-device shadow-sm d-none d-md-block"></div>
        <div class="product-device product-device-2 shadow-sm d-none d-md-block"></div>
    </div>
}
