@using WebMVC.Models.Entity;
@model List<EducationImage>

@foreach (var item in Model)
{
    <section id="education" class="img-fluid" style="position:relative; width: contain; height: 90vh; background-image: url(/image/@item.ImageName); background-attachment:fixed; background-size:cover; background-repeat:no-repeat;">
        @Html.Action("Education")
    </section>
}
